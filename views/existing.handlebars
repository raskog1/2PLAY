<div class="header">
  <header>
    <h1 id="logoText">2PLAY</h1>
    <a class="navbar-brand" href="/logout">
      <img src="images/cassette_tape3.png" class="navLogo" width="150" height="95" alt="2PlayLogo" />
    </a>
  </header>
</div>
<div class="container vertical-center">
  <div class="row">
    <div class="col-xs-12 col-md-3">
      <a href="/new">
        <img src="images/recordbutton.png" alt="New Playlist" class="mediaButton" id="mediaButton1" />
      </a>
    </div>
    <div class="col"></div>
    <div class="col-xs-12 col-md-3">
      <a href="/incomplete">
        <img src="images/pausebutton.png" alt="Unfinished Playlist" class="mediaButton" id="mediaButton2" />
      </a>
    </div>
    <div class="col"></div>
    <div class="col-xs-12 col-md-3">
      <a href="/complete">
        <img src="images/playbutton.png" alt="View Finished Playlist" class="mediaButton" id="mediaButton3" />
      </a>
    </div>
  </div>
</div>


<!-- Added from Anthony's slack FOR TESTING ONLY-->
<div class="container songsToRate">
  <!--Row was here-->
  <div class="pilot" data-user="pilot">
    {{!-- <h4>Songs that Pilot Needs to Rate</h4> --}}
  </div>

  <div class="coPilot" data-user="coPilot">
    {{!-- <h4>Songs that CoPilot Needs to Rate</h4> --}}
  </div>
</div>
</div>

<div class="container suggestedSongs">
  <div class="row">
    <div class="results">
      <!--Put the info for whatever button clicked here(new playlist, unfinished playlists, completed playlists)-->
    </div>
    <div class="container formContainer">
      <div class="row">
        <div class="container">
          <form class="login">
            <div class="form-group">
              <label for="songSearchInput">SONG NAME</label>
              <input type="text" class="form-control" id="songSearchInput"
                placeholder="What song are you looking for?" />
            </div>
            <div class="form-group">
              <label for="artistSearchInput">ARTIST NAME</label>
              <input type="text" class="form-control" id="artistSearchInput" placeholder="Artist" />
            </div>
            <div class="container vertical-center searchButtonContainer">
              <button id="search" type="submit" class="btn loginButton">
                <svg width="180px" height="60px" viewBox="0 0 180 60" class="border">
                  <polyline points="179,1 179,59 1,59 1,1 179,1" class="bg-line" />
                  <polyline points="179,1 179,59 1,59 1,1 179,1" class="hl-line" />
                </svg>
                <span>Search</span>
              </button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
<footer>
  <div class="generate vertical-center">
    <button id="generateComplete" type="submit" class="btn loginButton" style="display: none;">Generate
      Playlist</button>
  </div>
</footer>


